template <class T>
void bTree::insert(const std::pair<int,T> &pear)
{
	node* itr = root;
	while (!itr->isLeaf()){
		itr = itr->getNode(pear);
	}
	if (itr->getNumKeys() == 0){
		
	}
}

template <class T>
bool btree::search(int k)
{
	bool boo=false;
	if (root!=nullptr) {
		boo=root->search(k);
	}
	return boo;
}
